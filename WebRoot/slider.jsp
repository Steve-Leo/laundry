<!DOCTYPE html>
<%@ page language="java" contentType="text/xml; charset=UTF-8" pageEncoding="UTF-8"%>

<center style="padding-top:60px;">
<section id="rt-showcase-surround">
    <div id="rt-showcase" class="slider-container rt-overlay-dark">
        <div class="rt-container slider-container">
            <div class="rt-grid-12 rt-alpha rt-omega">
                
                <link rel="stylesheet" href="css/style.css">
                <!-- <link rel="stylesheet" href="css/ie.css">
                <script type="text/javascript" src="script/ie.js"></script> -->

                <div class="csslider1 autoplay">
                    <input name="cs_anchor1" autocomplete="off" id="cs_slide1_0" type="radio" class="cs_anchor slide" >
                    <input name="cs_anchor1" autocomplete="off" id="cs_slide1_1" type="radio" class="cs_anchor slide" >
                    <input name="cs_anchor1" autocomplete="off" id="cs_slide1_2" type="radio" class="cs_anchor slide" >
                    <input name="cs_anchor1" autocomplete="off" id="cs_play1" type="radio" class="cs_anchor" checked>
                    <input name="cs_anchor1" autocomplete="off" id="cs_pause1" type="radio" class="cs_anchor" >
                    <ul>
                        <div style="width: 100%; visibility: hidden; font-size: 0px; line-height: 0;">
                            <img src="image/laundry_d2.jpg" style="width: 100%;">
                        </div>
                        <li class="num0 img">
                            <img src="image/laundry_d2.jpg" alt="Clouds" title="Clouds" />
                        </li>
                        <li class="num1 img">
                            <img src="image/laundry_d3.jpg" alt="Typewriter" title="Flower" />
                        </li>
                        <li class="num2 img">
                            <img src="image/laundry_d6.jpg" alt="Bicycle" title="sky" />
                        </li>
                    
                    </ul>
                    <div class="cs_description">
                        <label class="num0">
                            <span class="cs_title"><span class="cs_wrapper">Clouds</span></span>
                            
                        </label>
                        <label class="num1">
                            <span class="cs_title"><span class="cs_wrapper">Flower</span></span>
                            
                        </label>
                        <label class="num2">
                            <span class="cs_title"><span class="cs_wrapper">Sky</span></span>
                            
                        </label>
                    </div>
                    
                    <div class="cs_arrowprev">
                        <label class="num0" for="cs_slide1_0"></label>
                        <label class="num1" for="cs_slide1_1"></label>
                        <label class="num2" for="cs_slide1_2"></label>
                    </div>
                    <div class="cs_arrownext">
                        <label class="num0" for="cs_slide1_0"></label>
                        <label class="num1" for="cs_slide1_1"></label>
                        <label class="num2" for="cs_slide1_2"></label>
                    </div>
                    
                    <div class="cs_bullets">
                        <label class="num0" for="cs_slide1_0">
                            <span class="cs_point"></span>
                            <span class="cs_thumb"><img src="image/small_d2.jpg" alt="Clouds" title="Clouds" /></span>
                        </label>
                        <label class="num1" for="cs_slide1_1">
                            <span class="cs_point"></span>
                            <span class="cs_thumb"><img src="image/small_d3.jpg" alt="Flower" title="Flower" /></span>
                        </label>
                        <label class="num2" for="cs_slide1_2">
                            <span class="cs_point"></span>
                            <span class="cs_thumb"><img src="image/small_d6.jpg" alt="Sky" title="Sky" /></span>
                        </label>
                    </div>
                    
                </div>

            </div>
            <div class="clear"></div>
        </div>
    </div>
</section>        <!-- /slider -->
</center>

